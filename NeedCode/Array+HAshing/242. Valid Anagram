//242. Valid Anagram


class Solution {
    public boolean isAnagram(String s, String t) {
      char[]ch=s.toCharArray();
      char[]str=t.toCharArray();
      Arrays.sort(ch);
      Arrays.sort(str);
      if(ch.length!=str.length){
          return false;
      }
      for(int i=0;i<ch.length;i++){
          if(ch[i]!=str[i]){
              return false;
          }
      }
      return true;
 }}
